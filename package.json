{
  "name": "@safepeek/utils",
  "version": "1.0.0",
  "description": "A package for typings and other utilities shared across SafePeek repositories.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ignore-pattern 'dist/'",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}'",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run format && pnpm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/safepeek/utils.git"
  },
  "author": "Anthony Collier <hello@acollier.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/safepeek/utils/issues"
  },
  "homepage": "https://github.com/safepeek/utils#readme",
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.3.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "dependencies": {
    "discord-api-types": "^0.37.84",
    "slash-create": "^6.1.3"
  }
}
